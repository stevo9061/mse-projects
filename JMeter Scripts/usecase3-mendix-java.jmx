          <stringProp name="parameters"></stringProp>
          <stringProp name="script">def statuses = [&quot;To_Do&quot;, &quot;Running&quot;, &quot;Review&quot;, &quot;Done&quot;]
def priority = [&quot;High&quot;, &quot;Medium&quot;, &quot;Low&quot;]

def rnd = new Random()
def randomIndexStatus = rnd.nextInt(statuses.size())
def randomIndexPriority = rnd.nextInt(priority.size())

def randomStatus = statuses[randomIndexStatus]
def randomPriority = priority[randomIndexPriority]

// Speichern der dynamischen Werte in JMeter-Variablen
vars.put(&quot;title&quot;, &quot;title&quot; + (ctx.getThreadNum() + 1));
vars.put(&quot;description&quot;, &quot;description&quot; + (ctx.getThreadNum() + 1));
vars.put(&quot;dueDate&quot;, &quot;${__timeShift(yyyy-MM-dd&apos;T&apos;HH:mm:ss,,P1D,)}&quot;)
vars.put(&quot;priority&quot;, randomPriority);
vars.put(&quot;status&quot;, randomStatus);
</stringProp>
