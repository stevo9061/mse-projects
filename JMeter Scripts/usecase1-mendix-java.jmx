          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.io.FileWriter;
import java.io.IOException;

FileWriter writer = null;

try {
    writer = new FileWriter(&quot;users.csv&quot;, true); // true = Append-Modus
    String username = vars.get(&quot;username&quot;); // Benutzername aus den Variablen holen
    String password = vars.get(&quot;password&quot;); // Passwort aus den Variablen holen

    // Log-Ausgaben zur Überprüfung der Werte
    log.info(&quot;Generated Username: &quot; + username);
    log.info(&quot;Generated Password: &quot; + password);

    if (username != null &amp;&amp; password != null) {
        writer.write(username + &quot;,&quot; + password + &quot;\n&quot;);
        log.info(&quot;Benutzer erfolgreich in Datei geschrieben: &quot; + username + &quot;, &quot; + password);
    } else {
        log.error(&quot;Benutzerdaten fehlen: username=&quot; + username + &quot;, password=&quot; + password);
    }
} catch (IOException e) {
    log.error(&quot;Fehler beim Schreiben in users.csv: &quot; + e.getMessage());
} finally {
    if (writer != null) {
        writer.close();
    }
}
</stringProp>
          </BeanShellPostProcessor>
