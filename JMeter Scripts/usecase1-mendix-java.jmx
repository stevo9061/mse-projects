          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.io.FileWriter;
import java.io.IOException;

FileWriter writer = null;

try {
    writer = new FileWriter(&quot;signup-users.csv&quot;, true); // true = Append-Modus
    String email = vars.get(&quot;emailAddress&quot;); // email aus den Variablen holen
    String userFullName = vars.get(&quot;userFullName&quot;); // userFullName aus den Variablen holen

    // Log-Ausgaben zur Überprüfung der Werte
    log.info(&quot;Generated EMail: &quot; + email);
    log.info(&quot;Generated user: &quot; + userFullName);

    if (email != null &amp;&amp; userFullName != null) {
        writer.write(email + &quot;,&quot; + userFullName + &quot;\n&quot;);
        log.info(&quot;Benutzer erfolgreich in Datei geschrieben: &quot; + email + &quot;, &quot; + userFullName);
    } else {
        log.error(&quot;Benutzerdaten fehlen: username=&quot; + email + &quot;, password=&quot; + userFullName);
    }
} catch (IOException e) {
    log.error(&quot;Fehler beim Schreiben in signup-users.csv: &quot; + e.getMessage());
} finally {
    if (writer != null) {
        writer.close();
    }
}
</stringProp>
